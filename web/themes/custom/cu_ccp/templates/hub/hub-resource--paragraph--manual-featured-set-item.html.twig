{#
/**
 * @file
 * Default theme file to display a hub_resource item.
 *
 * Available variables:
 * - content: hub resource content.
 *
 * @see template_preprocess_hub_resource()
 *
 * @ingroup themeable
 */
#}
{%
  set classes = [
  'hub-resource',
  'hub-resource--type-' ~ resource_type|clean_class,
]
%}

{%
  set fields = {
  'field_mfsi_link': content.field_mfsi_link['#elements'],
  'field_mfsi_short_text': content.field_mfsi_short_text,
  'field_mfsi_image': content.field_mfsi_image,
} %}

{% block resource %}
  {% include '@cu2019_grad/paragraph/paragraph--manual-featured-set-item.html.twig' with {
    'is_published': true,
    'bundle': resource_bundle,
    'content': fields,
    'paragraph': resource_obj,
    'mfs_icon': mfs_icon,
    'mfs_type': mfs_type,
    'view_mode': view_mode,
  } %}
{% endblock resource %}
