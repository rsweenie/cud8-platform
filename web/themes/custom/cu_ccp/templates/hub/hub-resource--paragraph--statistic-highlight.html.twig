{#
/**
 * @file
 * Default theme file to display a hub_resource item.
 *
 * Available variables:
 * - content: hub resource content.
 *
 * @see template_preprocess_hub_resource()
 *
 * @ingroup themeable
 */
#}
{%
  set classes = [
  'hub-resource',
  'hub-resource--type-' ~ resource_type|clean_class,
  'stat_hl_io'
]
%}
<article{{ attributes.addClass(classes) }}>
  {% if content.field_hub_stat_highlight_ref %}
    {{ content.field_hub_stat_highlight_ref }}
  {% endif %}
</article>
