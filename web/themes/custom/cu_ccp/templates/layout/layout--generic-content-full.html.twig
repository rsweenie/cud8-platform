{#
/**
 * @file
 * Displays the generic content layout.
 *
 * Available variables:
 * - content: The content for this layout.
 * - attributes: HTML attributes for the layout <div>.
 *
 * @ingroup themeable
 */
#}
{% if content %}
  <section {{ attributes.addClass('layout--generic-content') }}>
  {% if content.main %}
    <div {{ region_attributes.main.addClass('layout__region', 'layout__region--main') }}>
      {{ content.main }}
    </div>
  {% endif %}
  </section>
{% endif %}
